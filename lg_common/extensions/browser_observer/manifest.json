{
  "name":           "browser_observer",
  "version":        "0.1",
  "description":    "Extension that acts as a bridge between webNavigation and ROS",
  "default_locale": "en",
  "background": {
    "persistent": false,
    "scripts": [
                "navigation_collector.js",
                "background.js"
               ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions":    [
    "webNavigation", "storage", "<all_urls>"
  ],
  "manifest_version": 2
}
