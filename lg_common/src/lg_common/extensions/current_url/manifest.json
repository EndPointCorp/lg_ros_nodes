{
    "manifest_version": 3,
    "name": "current_url",
    "version": "0.2",
    "description": "Publish data about current url",
    "permissions": [
      "tabs"
    ],
    "host_permissions": [
      "ws://localhost:*/",
      "wss://localhost:*/"
    ],
    "background": {
      "service_worker": "current_url.js"
    },
    "action": {
      "default_title": "Send URL to ROS"
    }
}
